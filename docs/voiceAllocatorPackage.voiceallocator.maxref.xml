<?xml version='1.0' encoding='utf-8' standalone='yes'?>


<!-- DO NOT EDIT THIS FILE ... YOU WILL LOSE YOUR WORK -->


<c74object name='voiceAllocatorPackage.voiceallocator' category='velocities,  pitches,  etc.'>

	<digest>voice allocator for poly~ object </digest>
	<description>voice allocator for poly~ object </description>


	<!--METADATA-->

	<metadatalist>
		<metadata name='author'>Jan Godde, Edis Ludwig </metadata>
		<metadata name='tag'>velocities</metadata>
		<metadata name='tag'>pitches</metadata>
		<metadata name='tag'>etc.</metadata>
	</metadatalist>


	<!--ARGUMENTS-->

	<objarglist>

	</objarglist>


	<!--MESSAGES-->

	<methodlist>

		<method name='endall'>
			<digest>send message to release all voices </digest>
			<description>send message to release all voices </description>
		</method>

		<method name='endhold'>
			<digest>End hold notes 0 = all, 1 = last, 2 = first </digest>
			<description>End hold notes 0 = all, 1 = last, 2 = first </description>
		</method>

		<method name='scale_def'>
			<digest>scale_def [index, value] </digest>
			<description>scale_def [index, value] </description>
		</method>

		<method name='list'>
			<digest>midipitch, velocity, channel </digest>
			<description>midipitch, velocity, channel </description>
		</method>

		<method name='printscale'>
			<digest>Print scalearray to the max console </digest>
			<description>Print scalearray to the max console </description>
		</method>

		<method name='print'>
			<digest>Print info to the max console </digest>
			<description>Print info to the max console </description>
		</method>

	</methodlist>


	<!--ATTRIBUTES-->

	<attributelist>

		<attribute name='basefreq' get='1' set='1' type='float64' size='1' >
			<digest>Standard A, Default: 440 hz </digest>
			<description>Standard A, Default: 440 hz </description>
		</attribute>

		<attribute name='debug' get='1' set='1' type='long' size='1' >
			<digest>Debug on / off </digest>
			<description>Debug on / off </description>
		</attribute>

		<attribute name='hold' get='1' set='1' type='long' size='1' >
			<digest>Hold on / off </digest>
			<description>Hold on / off </description>
		</attribute>

		<attribute name='mono' get='1' set='1' type='long' size='1' >
			<digest>Monophony on / off </digest>
			<description>Monophony on / off </description>
		</attribute>

		<attribute name='mono_mode' get='1' set='1' type='long' size='1' >
			<digest>Choose Mono Mode: Last Note, Low Note, High Note </digest>
			<description>Choose Mono Mode: Last Note, Low Note, High Note </description>
		</attribute>

		<attribute name='output_mode' get='1' set='1' type='long' size='1' >
			<digest>Output Midi Notes or Frequencies </digest>
			<description>Output Midi Notes or Frequencies </description>
		</attribute>

		<attribute name='scale_def_mode' get='1' set='1' type='long' size='1' >
			<digest>Define Scale by Midi Note or by Frequency </digest>
			<description>Define Scale by Midi Note or by Frequency </description>
		</attribute>

		<attribute name='scale_fill' get='1' set='1' type='long' size='1' >
			<digest>Fill notes that are non-defined in scale_def message with MTOF </digest>
			<description>Fill notes that are non-defined in scale_def message with MTOF </description>
		</attribute>

		<attribute name='scalearray_maxsize' get='1' set='1' type='long' size='1' >
			<digest>Set maximum number of entries in scale array (default 127) </digest>
			<description>Set maximum number of entries in scale array (default 127) </description>
		</attribute>

		<attribute name='sequencer_steals' get='1' set='1' type='long' size='1' >
			<digest>Sequencer steals on / off </digest>
			<description>Sequencer steals on / off </description>
		</attribute>

		<attribute name='steal' get='1' set='1' type='long' size='1' >
			<digest>Steal on / off </digest>
			<description>Steal on / off </description>
		</attribute>

		<attribute name='steal_hold' get='1' set='1' type='long' size='1' >
			<digest>Steal Hold Notes on / off </digest>
			<description>Steal Hold Notes on / off </description>
		</attribute>

		<attribute name='voices' get='1' set='1' type='long' size='1' >
			<digest>Number of voices, default: 4 </digest>
			<description>Number of voices, default: 4 </description>
		</attribute>

	</attributelist>


	<!--RELATED-->

	<seealsolist>
		<seealso name='poly~' />
	</seealsolist>


</c74object>
